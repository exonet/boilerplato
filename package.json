{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "A simple react boilerplate",
  "scripts": {
    "clean": "rm -rf build/server && rm -rf build/static/css && rm -rf build/static/js",
    "build:client": "NODE_ENV=production webpack --progress --config webpack.config.js",
    "build:server": "NODE_ENV=production webpack --progress --config webpack.config.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "lint": "eslint app --ext js --ext jsx",
    "start:dev": "NODE_ENV=development node ./app/server",
    "start": "node ./build/server/index"
  },
  "author": "Robbin Janssen",
  "license": "ISC",
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "8.0.1",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.11",
    "dotenv": "^5.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-cli": "^2.1.4",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "emotion": "^9.2.4",
    "express": "^4.16.3",
    "jira-connector": "^2.8.0",
    "mdi-react": "^3.4.0",
    "moment": "^2.22.1",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-emotion": "^9.2.4",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "spectacle": "^4.2.3",
    "superagent": "^3.8.3"
  }
}
